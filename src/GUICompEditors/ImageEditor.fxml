<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="459.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUICompEditors.ImageEditorController">
    <stylesheets>
        <URL value="@../GUICompEditors/compeditor.css" />
    </stylesheets>
   <children>
      <GridPane layoutX="-1.0" prefHeight="394.0" prefWidth="451.0" styleClass="grid">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="436.0" minWidth="10.0" prefWidth="436.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="263.0" minHeight="10.0" prefHeight="263.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="125.0" minHeight="0.0" prefHeight="73.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="75.0" minHeight="10.0" prefHeight="58.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ImageView id="imgView" fitHeight="262.0" fitWidth="416.0" pickOnBounds="true" preserveRatio="true" styleClass="mainFxmlClass">
               <image>
                  <Image url="@../ProgramImg/DefaultImg.jpg" />
               </image></ImageView>
            <TextArea prefHeight="200.0" prefWidth="200.0" styleClass="mainFxmlClass" text="Type your caption here......" GridPane.rowIndex="1">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></TextArea>
            <HBox prefHeight="100.0" prefWidth="200.0" styleClass="hbox" GridPane.rowIndex="2">
               <children>
                  <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#select" styleClass="button" text="Select Image">
                     </Button>
                  <Pane prefHeight="41.0" prefWidth="147.0" />
                  <Button id="button" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#okClicked" styleClass="button" text="OK">
                   </Button>
                  <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#cancelClicked" styleClass="button" text="Cancel">
                     </Button>
               </children>
            </HBox>
         </children>
      </GridPane>
   </children>
</AnchorPane>
